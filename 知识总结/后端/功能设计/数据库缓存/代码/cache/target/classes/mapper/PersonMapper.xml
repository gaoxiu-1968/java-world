<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.zhangmingke.cache.dao.PersonDao">
    <select id="getPerson" resultType="top.zhangmingke.cache.entity.Person">
        select
        name,
        age
        from
        person
        where   
        id = #{id}
    </select>
</mapper>